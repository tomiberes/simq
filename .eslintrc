{
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
  },
  "plugins": [
  ]
}
